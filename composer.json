{
    "name": "amazeelabs/silverback",
    "description": "Scaffolder for Amazee Silverback projects.",
    "type": "package",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Philipp Melab<philipp.melab@gmail.com>",
            "role": "Master of desaster"
        }

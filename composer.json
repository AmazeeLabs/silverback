{
    "name": "amazeelabs/silverback",
    "description": "Scaffolder for Amazee Silverback projects.",
    "type": "package",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Philipp Melab<philipp.melab@gmail.com>",
            "role": "Master of desaster"
        }
    ],
    "require": {
        "vlucas\/phpdotenv": "^2.4",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/better_normalizers": "1.x-dev",
        "drupal/console": "^1.0.2",
        "drupal/core": "8.7.x-dev",
        "drupal/default_content": "1.x-dev",
        "drupal/entity_usage": "^2.0@alpha",
        "drupal/redirect": "^1.2",
        "drupal/typed_data": "1.x-dev",
        "drush/drush": "^9.0.0",
        "drupal/moderation_state_columns": "dev-1.x",
        "drupal/moderation_state_buttons_widget": "dev-1.x",
        "drupal/webpack_react": "dev-1.x",
        "drupal/admin_toolbar": "^1",
        "drupal/moderation_state_permissions": "dev-1.x"
    },
    "require-dev": {
        "webflo\/drupal-core-require-dev": "8.7.x-dev"
    },
    "bin": [ "bin/silverback" ],
    "autoload": {
        "psr-4": {
            "AmazeeLabs\\Silverback\\": "src/AmazeeLabs/Silverback"
        }
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "[PP-1] Entity type CRUD operations must use the last installed entity type and field storage definitions (#2976035 - 48)": "https://www.drupal.org/files/issues/2018-11-12/2976035-48.patch",
                "[PP-1] Convert taxonomy terms to be revisionable (#2880149 - 197)": "https://www.drupal.org/files/issues/2018-11-12/diff-2880149-197--2984782-29.txt",
                "Convert custom menu links to be revisionable (#2880152 - 30)": "https://www.drupal.org/files/issues/2018-10-10/2880152-30-for-review.patch"
            }
        },
        "enable-patching": true
    }
}
